{
  "env": {
    "browser": true,
    "es6": true
  },
  "plugins": [
    "prettier"
  ],
  "extends": [
    "airbnb-base",
    "plugin:prettier/recommended",
    "prettier"
  ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "rules": {
    "strict": 0,
    "no-console": 1,
    "no-plusplus": 0,
    "no-restricted-syntax": 0,
    "no-param-reassign": 0,
    "no-continue": 0,
    "consistent-return": 1
  }
}
